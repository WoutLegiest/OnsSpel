<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="320.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RegisterController">
   <children>
      <Button fx:id="buttonRegister" layoutX="129.0" layoutY="235.0" mnemonicParsing="false" onAction="#register" text="Register" />
      <Label layoutX="45.0" layoutY="35.0" text="BAD ASS MEMORY GAME" textOverrun="LEADING_ELLIPSIS" underline="true">
         <font>
            <Font name="Comic Sans MS Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="textUsername" layoutX="131.0" layoutY="95.0" />
      <PasswordField fx:id="textPassword" layoutX="131.0" layoutY="139.0" />
      <Label layoutX="33.0" layoutY="100.0" text="Username:" />
      <Label layoutX="33.0" layoutY="144.0" text="Password:" />
      <TextField fx:id="textEmail" layoutX="131.0" layoutY="186.0" />
      <Label layoutX="33.0" layoutY="191.0" text="Email:" />
      <Button fx:id="buttonBackToLogin" layoutX="107.0" layoutY="278.0" mnemonicParsing="false" onAction="#toLogin" text="Terug naar login" />
   </children>
</AnchorPane>
